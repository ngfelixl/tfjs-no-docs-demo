{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "outDir": "./dist",
    "rootDirs": ["./src","./test"],
    "sourceRoot": ".",
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  },
  "exclude": [
    "./dist"
  ]
}